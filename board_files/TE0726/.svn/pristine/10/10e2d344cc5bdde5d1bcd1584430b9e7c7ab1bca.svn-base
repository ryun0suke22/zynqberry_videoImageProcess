<board schema_version="2.1" vendor="trenz.biz" name="TE0726_01" display_name="ZYNQ-7 TE0726_01. SPRT PCB: REV01." url="wiki.trenz-electronic.de" preset_file="preset.xml">
  <images>
    <image name="te0726_board.jpg" display_name="ZYNQ-7 TE0726_01 BOARD" sub_type="board">
      <description>ZYNQ-7 TE0726_01 Board File Image</description>
    </image>
  </images>
<!-- ##################################################################### -->
<!-- ##Board PCB Revision -->
<!-- ##Currently revision with highest id is only displayed in Vivado. Write supported revisions to description too!-->
